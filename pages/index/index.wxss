.ResumeContainer{
  background: url('data:image/png;base64,') repeat;
}
.banner {
  position: relative;
}

.banner .bannerBg {
  width: 100%;
  height: 300px;
}

.banner .bannerOverlay {
  width: 100%;
  height: 300px;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(41, 41, 41, 0.3);
  z-index: 1;
}

.banner .info {
  color: #fff;
  position: absolute;
  z-index: 3;
  text-align: center;
  width: 100%;
  top: 85px;
}

.banner .text {
  font-size: 1.2em;
}

.banner .titleResume {
  padding-bottom: 4px;
  margin-bottom: 5px;
  font-size: 1.2em;
  font-weight: 900;
}

.banner .titleResume::after {
  border-bottom: 2px solid #fff;
  content: ' ';
  width: 100px;
  height: 2px;
  display: block;
  margin-left: 38%;
}

.banner .enName {
  font-size: 0.9em;
}

.banner .title {
  font-size: 0.8em;
}
.banner .head{
  position: absolute;
  top:260px;
  left:40%;
  width:80px;
  height: 80px;
  border:2px solid #fff;
  border-radius: 50%;
  z-index: 4;
}
.area{
  padding:10px;
  color:#3d3b3b;
}
.area>.title{
  padding-bottom:10px;
  border-bottom:1px solid #3d3b3b;
  margin-bottom: 10px;
}
.area>.title::before{
  content:  ' ';
  display: inline-block;
  border-left: 4px solid #3d3b3b;
  width:1px;
  height: 20px;
  margin-left: 5px;
  margin-top: -4px;
  vertical-align: middle;
}
.aboutMe{
  margin-top: 30px;
}
.aboutMe .iconArea{
  margin-bottom: 10px;
}
.aboutMe .iconArea image{
  width: 25px;
  height:25px;
  vertical-align: middle;
  margin-right: 10px;
}
